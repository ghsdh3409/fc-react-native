{
  "name": "MovieReminder",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "code-push-staging-ios": "appcenter codepush release-react -a daehoon/MovieReminder-iOS -d Staging",

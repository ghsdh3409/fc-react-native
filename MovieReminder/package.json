    "code-push-prod-ios": "appcenter codepush release-react -a daehoon/MovieReminder-iOS -d Production",
    "code-push-prod-android": "appcenter codepush release-react -a daehoon/MovieReminder-Android -d Production",
    "get-version-number-js": "node ./scripts/get-version-number-js.js",
    "get-version-number-android": "node ./scripts/get-version-number-android.js android/app/build.gradle",
    "get-build-number-android": "node ./scripts/get-build-number-android.js android/app/build.gradle"
  },
  "dependencies": {
    "@notifee/react-native": "^7.1.0",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/native-stack": "^6.9.1",
    "@tanstack/react-query": "^4.16.1",
    "axios": "^1.1.3",
    "moment": "^2.29.4",
    "open-color": "^1.9.1",
    "react": "18.1.0",
    "react-native": "0.70.5",
    "react-native-code-push": "^7.0.5",
    "react-native-google-mobile-ads": "^8.2.2",
    "react-native-purchases": "^5.3.0",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-screens": "^3.18.2",
    "react-native-vector-icons": "^9.2.0"
